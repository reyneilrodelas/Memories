{
  "version": 2,
  "builds": [
    {
      "src": "dist/index.cjs",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "dist/index.cjs"
    },
    {
      "src": "/(.*)",
      "dest": "dist/index.cjs"
    }
  ],
  "outputDirectory": "dist",
  "buildCommand": "npm run build",
  "installCommand": "npm install"
}
